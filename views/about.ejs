    <style>
        .about {
            font-family: 'Helvetica Neue', Arial, sans-serif;
            background-color: #212121;
            color: #ffffff;
            line-height: 1.6;
            padding: 20px;
            margin: 0;
        }

        .team-section, .faq-section {
            max-width: 1200px;
            margin: 0 auto 40px;
        }

        .team-section h1, .faq-section h2 {
            text-align: center;
            font-size: 32px;
            margin-bottom: 20px;
        }

        .team-section p, .faq-section p {
            text-align: center;
            max-width: 800px;
            margin: 0 auto 20px;
            font-size: 16px;
        }

        .team-grid {
            display: grid;
            grid-template-columns: repeat(3, 1fr);
            gap: 20px;
            padding: 20px;
        }

        .team-member {
            background: #3B3B3B;
            border: 1px solid #212121;
            padding: 20px;
            text-align: center;
            box-shadow: 0px 4px 8px rgba(0,0,0,0.1);
            border-radius: 8px;
        }

        .team-member img {
            width: 100%;
            height: 200px;
            object-fit: cover;
            border-top-left-radius: 8px;
            border-top-right-radius: 8px;
        }

        .team-member h2 {
            margin-top: 15px;
            font-size: 18px;
        }

        .team-member p {
            font-size: 14px;
            margin: 10px 0 0;
            line-height: 1.5;
            text-align: left;
            padding: 0 5px;
        }

        .faq-section {
            padding: 20px;
        }

        .faq-container {
            display: flex;
            flex-wrap: wrap;
            align-items: flex-start;
            max-width: 1200px;
            width: 100%;
        }

        .faq-text, .faq-list {
            flex: 1;
            margin: 1rem;
        }

        .faq-text h2 {
            font-size: 2rem;
            margin-bottom: 1rem;
            text-align: left;
        }

        .faq-text p {
            margin-bottom: 2rem;
            text-align: left;

        }

        .faq-text button {
            background-color: white;
            color: black;
            border: none;
            padding: 0.5rem 1rem;
            border-radius: 5px;
            cursor: pointer;
            font-size: 1rem;
        }

        .faq-list {
            margin-left: 0;
        }

        .faq-item {
            background-color: #333;
            margin-bottom: 0.5rem;
            border-radius: 8px;
            overflow: hidden;
        }

        .faq-item-header {
            padding: 1rem;
            cursor: pointer;
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

        .faq-item-header h3 {
            margin: 0;
            font-size: 1rem;
        }

        .faq-item-header.active h3 {
            color: #f4af1b;
        }

        .faq-item-content {
            max-height: 0;
            overflow: hidden;
            transition: max-height 0.3s ease-out;
            background-color: #444;
        }

        .faq-item-content p {
            padding: 1rem;
            margin: 0;
        }

        .faq-item-header.active + .faq-item-content {
            max-height: 200px;
        }

        .icon {
            font-size: 1.5rem;
        }

        .icon.open {
            transform: rotate(45deg);
        }

        @media (max-width: 768px) {
            .team-grid {
                grid-template-columns: 1fr;
            }
            .faq-container {
                flex-direction: column;
            }
            .faq-text, .faq-list {
                flex: none;
                width: 100%;
            }
            .faq-text h2 {
                font-size: 1.5rem;
            }
            .faq-text p {
                font-size: 0.9rem;
            }
            .faq-text button {
                font-size: 0.9rem;
            }
            .faq-item-header h3 {
                font-size: 0.9rem;
            }
            .icon {
                font-size: 1.2rem;
            }
        }
    </style>
</head>
