<style>
        body {
    font-family: Arial, sans-serif;
    margin: 0;
    padding: 0;
    box-sizing: border-box;
    background-color: #121212; /* Dark background */
    color: #f5f5f5; /* Light gray text */
}

.intro-text-unique {
    text-align: center;
    margin: 40px 20px;
    color: #f5f5f5; /* Light gray text */
}

.intro-text-unique h1 {
    color: #ffa500; /* Yellow-orange */
    font-size: 2.5em;
    margin-bottom: 20px;
    line-height: 1.3;
}

.intro-text-unique .highlight-unique {
    color: #ffffff; /* White color */
}

.intro-text-unique p {
    font-size: 1.2em;
    color: #f5f5f5; /* Light gray text */
    max-width: 800px;
    margin: 0 auto 60px;
    line-height: 1.6;
    text-align: justify;
}

.card-container-unique {
    display: flex;
    justify-content: space-around;
    margin: 0 20px 40px;
    flex-wrap: wrap;
}

.card-unique {
    border: 1px solid #ffa500; /* Yellow-orange border */
    padding: 20px;
    width: 45%;
    box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.5);
    margin-bottom: 40px;
    background-color: #1e1e1e; /* Dark card background */
}

.card-unique h3 {
    color: #ffa500; /* Yellow-orange */
    font-size: 1.7em;
    margin-bottom: 15px;
}

.card-unique .price-unique {
    font-weight: bold;
    margin-bottom: 20px;
    font-size: 1.1em;
    color: #f5f5f5; /* Light gray text */
}

.card-unique ul {
    list-style-type: disc;
    margin-left: 20px;
    margin-bottom: 20px;
    color: #f5f5f5; /* Light gray text */
}

.card-unique a {
    color: #ffa500; /* Yellow-orange links */
    text-decoration: none;
    font-weight: bold;
}

.card-unique a:hover {
    text-decoration: underline;
}

.slider-unique {
    position: relative;
    width: 100%;
    height: auto;
    overflow: hidden;
    margin-bottom: 20px;
}

.slides-unique {
    display: flex;
    transition: transform 0.5s ease-in-out;
}

.slider-unique img {
    width: 100%;
    height: auto;
}

.slider-controls-unique {
    position: absolute;
    top: 50%;
    width: 100%;
    display: flex;
    justify-content: space-between;
    transform: translateY(-50%);
    padding: 0 10px;
}

.slider-controls-unique .prev-unique,
.slider-controls-unique .next-unique {
    background-color: rgba(0, 0, 0, 0.5);
    color: #ffffff; /* White arrows */
    padding: 10px;
    cursor: pointer;
    border-radius: 50%;
    user-select: none;
}

@media screen and (max-width: 768px) {
    .card-unique {
        width: 100%;
    }
    
    .intro-text-unique h1 {
        font-size: 2em;
    }
    
    .intro-text-unique p {
        font-size: 1em;
        margin-bottom: 40px;
    }
}

   </style>

