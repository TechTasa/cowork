        @import url('https://fonts.googleapis.com/css2?family=Poppins:wght@400;600&display=swap');

        body {
            font-family: 'Poppins', sans-serif;
            margin: 0;
            padding: 0;
            box-sizing: border-box;
            background-color: #121212;
            color: #f4f4f4;
            overflow-x: hidden;
        }

       
        .intro-section {
            height: 100vh;
            background: url('/images/service-img.jpg') no-repeat center center/cover;
            color: #fff;
            text-align: center;
            display: flex;
            flex-direction: column;
            justify-content: center;
            position: relative;
            overflow: hidden;
        }

        .intro-section h1 {
            font-size: 4em;
            margin: 0;
            z-index: 1;
            position: relative;
        }

        .intro-section p {
            font-size: 1.5em;
            margin: 20px 0 0;
            z-index: 1;
            position: relative;
        }

        .services-container {
            padding: 100px 20px;
            position: relative;
            z-index: 10;
            display: flex;
            flex-wrap: wrap;
            justify-content: center;
        }

        .service-card {
            background: #1f1f1f;
            border-radius: 20px;
            overflow: hidden;
            width: 300px;
            height: 400px;
            margin: 20px;
            perspective: 1000px;
            position: relative;
            transition: transform 0.6s;
        }

        .service-card:hover {
            transform: rotateY(10deg) rotateX(10deg);
        }

        .service-card-inner {
            width: 100%;
            height: 100%;
            transition: transform 0.6s;
            transform-style: preserve-3d;
            position: relative;
        }

        .service-card:hover .service-card-inner {
            transform: rotateY(10deg) rotateX(10deg);
        }

        .service-card-front, .service-card-back {
            position: absolute;
            width: 100%;
            height: 100%;
            backface-visibility: hidden;
        }

        .service-card-front {
            background: #1f1f1f;
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            color: #fff;
            text-align: center;
        }

        .service-card-front img {
            width: 100%;
            height: 60%;
            object-fit: cover;
        }

        .service-card-back {
            background: #0073e6;
            color: #fff;
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            transform: rotateY(180deg);
            padding: 20px;
            text-align: center;
        }

        .service-card-back h2 {
            margin: 0;
            font-size: 1.5em;
        }

        .service-card-back p {
            margin: 10px 0 0;
            font-size: 1.2em;
        }


        @media (min-width: 768px) {
            .service-card {
                width: 350px;
                height: 450px;
            }
        }

        @media (min-width: 1024px) {
            .service-card {
                width: 400px;
                height: 500px;
            }
        }
    </style>
</head>
