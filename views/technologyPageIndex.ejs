        .container {
            max-width: 1200px;
            margin: 0 auto;
            padding: 20px;
        }

        .header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            border-bottom: 2px solid #444;
            padding-bottom: 10px;
        }

        .header h1 {
            margin: 0;
        }

        .price {
            font-size: 24px;
            background-color: #ffcc00;
            color: #333;
            padding: 5px 10px;
            border-radius: 5px;
            
        }

        .amenities-map {
            display: flex;
            justify-content: space-between;
            margin: 20px 0;
            flex-wrap: wrap;
        }

        .amenities {
            flex: 1;
            display: flex;
            flex-direction: column;
        }

        .amenities h2 {
            margin-bottom: 10px;
        }

        .amenities ul {
            list-style: none;
            padding: 0;
            display: flex;
            flex-wrap: wrap;
        }

        .amenities ul li {
            padding: 10px;
            border-radius: 5px;
            margin: 5px;
            display: flex;
            align-items: center;
            flex: 1 1 calc(33.333% - 10px); /* Three items per row */
        }

        .amenities ul li i {
            margin-right: 10px;
        }

        .map {
            flex: 1;
            margin-left: 20px;
            margin-top: 20px;
        }

        .map iframe {
            width: 100%;
            border-radius: 10px;
        }

        @media (max-width: 768px) {
            .header {
                flex-direction: column;
                align-items: flex-start;
            }

            .main-content {
                flex-direction: column;
            }

            .main-image {
                margin-right: 0;
                margin-bottom: 20px;
            }

            .location, .gallery, .services, .amenities-map {
                flex-direction: column;
            }

            .gallery img, .service-item {
                width: 100%;
                margin-bottom: 20px;
            }

            .map {
                margin-left: 0;
            }

            .amenities ul li {
                flex: 1 1 100%;
            }
        }

