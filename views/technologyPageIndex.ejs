        .features-wrapper {
            background-color: #3B3B3B;
            color: #fff;
            padding: 40px 20px;
            width: 100%;
            text-align: center;
            box-sizing: border-box;
        }

        .features-wrapper h2 {
            font-size: 2rem;
            margin-bottom: 20px;
        }

        .feature-cards {
            display: grid;
            grid-template-columns: repeat(5, 1fr); /* Five items per row */
            gap: 20px;
        }

        .feature-card {
            background-color: #000000;
            padding: 20px;
            border-radius: 8px;
            box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
            text-align: left;
        }

        .feature-card img {
            width: 50px;
            margin-bottom: 10px;
        }

        .feature-card h3 {
            font-size: 1rem;
            margin-bottom: 10px;
        }

        .feature-card p {
            font-size: 1rem;
            line-height: 1.5;
        }

        /* Responsive Styles */
        @media (max-width: 900px) {
            .feature-cards {
                grid-template-columns: repeat(2, 1fr); /* Two items per row */
            }
        }

        @media (max-width: 600px) {
            .feature-cards {
                grid-template-columns: 1fr; /* One item per row */
            }

