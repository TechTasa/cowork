    <style>
        /* General Styles */
        .making {
            font-family: 'Helvetica Neue', Arial, sans-serif;
            margin: 0;
            padding: 0;
            background-color: #212121;
            color: #333;
            display: flex;
            flex-direction: column;
            align-items: center;
            min-height: 100vh;
        }

        /* Main Content Styles */
        main {
            max-width: 800px;
            margin: 50px auto;
            padding: 20px;
            background-color: #212121;
            color: #fff;
        }

        h1 {
            text-align: center;
            font-size: 32px;
        }

        ol {
            list-style-type: none;
            padding: 0;
        }

        ol > li:not(:last-child) {
            margin-bottom: 20px;
        }

        h2 {
            font-size: 20px;
            margin-bottom: 10px;
        }

        p {
            line-height: 1.6;
        }

        /* Hero Section Styles */
        .hero-section {
            position: relative;
            width: 100%;
            height: 70vh;
            background-image: url('/images/bigoffice.jfif');
            background-size: cover;
            background-position: center;
            display: flex;
            justify-content: center;
            align-items: center;
        }

        .overlay {
            background-color: #212121;
            padding: 30px 40px;
            border-radius: 10px;
            color: white;
            max-width: 600px;
            width: 100%;
            box-sizing: border-box;
            text-align: center;
        }

        .overlay h2 {
            margin-bottom: 15px;
            font-size: 1.75rem;
            text-align: left;
        }

        .overlay p {
            margin-bottom: 20px;
            font-size: 1rem;
            text-align: left;
        }

        .form-group {
            display: flex;
            flex-wrap: wrap;
            gap: 10px;
            text-align: left;
        }

        .form-group input {
            flex: 1 1 calc(50% - 10px);
            padding: 10px;
            border-radius: 5px;
            border: 1px solid #3B3B3B;
            background-color: #3B3B3B;
            font-size: 1rem;
            box-sizing: border-box;
            font-family: 'Helvetica Neue', Arial, sans-serif;
        }

        .form-group input.full-width {
            flex: 1 1 100%;
        }

        .submit-container {
            display: flex;
            justify-content: flex-start;
            width: 100%;
            margin-top: 20px;
        }

        .submit-btn {
            padding: 8px 15px;
            background-color: #ffffff;
            border: none;
            border-radius: 5px;
            color: rgb(0, 0, 0);
            cursor: pointer;
            font-size: 0.9rem;
        }

        /* Responsive Styles */
        @media (max-width: 600px) {
            .overlay {
                padding: 20px;
            }

            .form-group input {
                flex: 1 1 100%;
            }
        }
    </style>
</head>
